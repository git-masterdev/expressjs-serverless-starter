{
  "name": "serverless-boilerplate-express-typescript",
  "version": "1.0.0",
  "description": "Serverless framework ExpressJS TypeScript",
  "scripts": {
    "dev": "cross-env NODE_ENV=development sls offline start --stage offline",
    "tail-log": "sls logs -f app -t",
    "deploy-prod": "cross-env NODE_ENV=production sls deploy --stage prod",
    "remove-prod": "cross-env NODE_ENV=production sls remove --stage prod",
    "clean": "rimraf dist .webpack .serverless",
    "lint": "eslint --ext .js,.ts .",
    "build-types": "tsc --noEmit --pretty",
    "test": "jest",
    "commit": "cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^5.1.1",
    "serverless-http": "^3.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/cz-commitlint": "^17.1.2",
    "@swc/jest": "^0.2.23",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^29.2.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "serverless": "^3.23.0",
    "serverless-bundle": "^5.5.0",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-offline": "^11.1.2",
    "supertest": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "author": "Ixartz (https://github.com/ixartz)"
}
